#include <stack>
#include <iostream>
#include <fstream>
using namespace std;
ifstream in("input.txt");
ofstream out("output.txt");
int main()
{
    stack <int> t1;
    stack <int> t2;
    int k, sum=0, count=0;
    while(in >> k)
        t1.push(k);
    if (!t1.empty()){
        t2.push(t1.top());
        sum += t1.top();
        count += 1;
        t1.pop();
    }
    while (!t1.empty()){
        if (t1.top()>= sum/count*1.0){
            t2.push(t1.top());
            sum += t1.top();
            count += 1;
            t1.pop();
        }
    }
    while (!t1.empty()){
        out << t2.top();
        t2.pop();
    }
    in.close();
    out.close();
    return 0;
}
