//в целочисленном одномерном массиве удалить все элементы, последняя цифра которых равна данной
#include <iostream>
using namespace std;

int main() {
	int n, c;
	cout << "Vvedite razmer massiva ";
	cin >> n;
	int* a = new int[n];
	cout << "Vvedite chislo ";
	cin >> c;

	for (int i = 0; i < n; i++) {
		cout << "a[" << i << "] = ";
		cin >> a[i];
		if (a[i] % 10 == c) {
			for (int j = i; j < n-1; j++) {
				a[j] = a[j + 1];
			}
			n--;
			i--;
		}
	}
	cout << endl;
	for (int i = 0; i < n; i++) {
		cout << a[i] << " ";
	}

	return 0;
}
