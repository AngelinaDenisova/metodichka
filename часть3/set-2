#include <iostream>
#include <set>
#include <string>
#include <cctype>
using namespace std;
int main() {
    setlocale (LC_ALL, "Russian");
    int n;
    cout << "Введите количество строк: ";
    cin >> n;
    cout << "Введите строки: ";
    for (int i = 0; i < n+1; i++) {
        string s;
        getline(cin, s);
        set<char> uppercase_chars;
        set<char> lowercase_chars;
        for (char c : s) {
            if (isupper(c)) {
                uppercase_chars.insert(c);
            } else if (islower(c)) {
                lowercase_chars.insert(c);
            }
        }
        set<char> result;
        for (char c : uppercase_chars) {
            if (lowercase_chars.find(tolower(c)) == lowercase_chars.end()) {
                result.insert(c);
            }
        }
        for (char c : result) {
            cout << c;
        }
        cout << endl;
    }
    return 0;
}

