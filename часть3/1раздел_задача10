
#include <iostream>
using namespace std;

int main()
{
	double x, y;
	setlocale(LC_ALL, "Russian");
	cout << "Введите координаты х и у" << endl;;
	cin >> x >> y;
	if ((y >= -100) and (y <= -x) and (y <= x)) {
		if ((y == -100) or (y == -x) or (y == x))
			cout << "На границе";
		else
			cout << "Да";
	}
	else {
		cout << "Нет";
	}
	return 0;
}
