#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	char x;
	cout << "Введите признак транспортного средства: a-автомобиль, v-велосипед, m-мотоцикл, s-самолет, p-поезд" << endl;
	cin >> x;
	switch (x) {
	case 'a':
		cout << "1228 км/ч";
		break;
	case 'v':
		cout << "268 км/ч";
		break;
	case 'm':
		cout << "350 км/ч";
	case 's':
		cout << "3540 км/ч";
		break;
	case 'p':
		cout << "603 км/ч";
		break;
	default:
		cout << "Введено неверное значение";
	}
	return 0;
}
