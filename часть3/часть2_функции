
#include <iostream>
using namespace std;

double k(int x) {
	return (x % 5 == 0 ? x / 5 : x - 1);
}

int main()
{
	int x, p;
	setlocale(LC_ALL, "Russian");
	cout << "Введите число" << endl;
	cin >> x;
	p = k(x);
	cout << p;
	return 0;
}
//тесты 
//х =0 результат:0
//х==100 результат: 20
//х=789 результат: 790
