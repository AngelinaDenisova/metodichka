
#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
void f(double x) {
	if (x <= 1)
		cout << "Функция не определена";
	else cout << sqrt(pow(x, 3) - 1) / sqrt(x * x - 1);
}
int main()
{
	setlocale(LC_ALL, "Russian");
	double a, b, h, x;
	cout << "a=";
	cin >> a;
	cout << "b=";
	cin >> b;
	cout << "h=";
	cin >> h;
	cout << "x\tf(x)\n";
	for (x = a; x <= b; x += h) {
		cout << x << "\t";
		cout << fixed << setprecision(2);
		f(x);
		cout << endl;
	}
	return 0;
}

тесты:
a=0, b=4; h=1
a=-10, b=0; h=2
a=100, b=200, h=10
