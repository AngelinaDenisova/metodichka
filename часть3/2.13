#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	char x;
	cout << "Введите признак транспортного средства: a-автомобиль, v-велосипед, m-мотоцикл, s-самолет, p-поезд" << endl;
	cin >> x;
	if (x == 'a')
		cout << "1228 км/ч";
	if (x == 'v')
		cout << "268 км/ч";
