//удалить из строки s все подстроки s1
#include <iostream> 
#include <string>
using namespace std;
int main()
{
    string s, s1;
    int c = -1;
    cout << "Vvedite stroky" << endl;
    getline(cin, s);
    cout << "Vvedite podstroky" << endl;
    getline(cin, s1);
    for (int i = 0; i < s.size() - 1; ++i) {
        c = s.find(s1);
        if (c != -1)
        {
            s.erase(c, s1.size());
        }
    }
    cout << s;
    return 0;
}
