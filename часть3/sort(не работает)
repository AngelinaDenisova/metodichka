#include <iostream>
#include <fstream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

struct Student {
    int group;
    string surname, name, otch;
    int year;
    vector<int> grades;
};

bool compare(Student a, Student b) {
    int sum_a = 0;
    int sum_b = 0;
    for (int i = 0; i < 5; i++) {
        sum_a += a.grades[i];
        sum_b += b.grades[i];
    }
    return sum_a > sum_b;
}

int main() {
    ifstream in("input.txt");
    vector<Student> students;
    while (!in.eof()) {
        Student s;
        in >> s.group >> s.surname >> s.name >> s.otch >> s.year;
        for (int i = 0; i < 5; i++) {
            int grade;
            in >> grade;
            s.grades.push_back(grade);
        }
        students.push_back(s);
    }
    in.close();
    sort(students.begin(), students.end(), compare);
    ofstream out("output.txt");
    for (int i = 0; i < students.size(); i++) {
        out << students[i].group << " " << students[i].surname << " " << students[i].name << " " << students[i].otch << " " << students[i].year << " ";
        for (int j = 0; j < 5; j++) {
            out << students[i].grades[j] << " ";
        }
        out << endl;
    }
    out.close();
    return 0;
}
