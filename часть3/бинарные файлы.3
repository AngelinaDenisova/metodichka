//создать файл и записать в него обратные натуральные числа. Вывести компоненты файла с порядковым номером, кратным трем
#include <fstream>
#include <iostream>
using namespace std;
int main()
{
    ofstream out("f.dat", ios::binary);
    int n, i;
    double k;
    cin >> n;
    for (i=1; i<=n; i++){
        k=1./i;
        if (i%3==0)
            out.write((char*)&k, sizeof(k));
    }
    out.close();
    ifstream in("f.dat", ios::binary);
    while (in.peek()!=EOF){
            in.read((char*)&k, sizeof(k));
            cout << k << " ";
    }
    in.close();
    return 0;
}
