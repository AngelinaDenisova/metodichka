//Используя механизм перегрузки функций, разработать 2 версии функции F, заголовки
#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
double const esp = 1e-12;
double f(double x) {
	double y;
	if (abs(x) < 3)
		y = sin(x);
	if (abs(x) >= 3 or abs(x) < 9)
		y = (sqrt(x * x + 1)) / (sqrt(x * x + 5));
	if (abs(x) >= 9)
		y = (sqrt(x * x + 1)) - (sqrt(x * x + 5));
	return y;
}
void f(double x, double& y) {
	f(x);
}
int main()
{
	double x, y;
	cin >> x;
	y = f(x);
	cout << y << endl;
	y = 0;
	f(x, y);
	cout << y << endl;
	return 0;
}
