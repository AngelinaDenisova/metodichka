#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
const double esp = 1e-12;
void f(double x, int a) {
	if (x+esp < a)
		cout << 0;
	if (x+esp > a)
		cout << (double)(x - a) / (x + a);
	if (abs(x-a) < esp)
		cout << 1;
}
int main()
{
	setlocale(LC_ALL, "Russian");
	double a, b, h, x;
	cout << "a=";
	cin >> a;
	cout << "b=";
	cin >> b;
	cout << "h=";
	cin >> h;
	cout << "x\tf(x)\n";
	for (x = a; x < b+esp; x += h) {
		cout << x << "\t";
		cout << fixed << setprecision(2);
		f(x, a);
		cout << endl;
	}
	return 0;
}

тесты:
а=1, b=10, h=1
a=10, b=100, 2
a=100, b=1000, h=100
