#include <iostream>
using namespace std;
int main() {
	int b1 = 1, b2 = 5, b, n;
	cout << "n= ";
	cin >> n;
	cout << "b1= " << b1 << endl << "b2 = " << b2 << endl;
	for (int i = 3; i <= n; i++) {
		if (i % 2 == 1) {
			b = b1 + b2;
			cout << "b" << i << "= " << b << endl;
			b1 = b2;
			b2 = b;
		}
		if (i % 2 == 0) {
			b = b2 - b1;
			cout << "b" << i << "= " << b << endl;
			b1 = b2;
			b2 = b;
		}
	}
	return 0;
}
